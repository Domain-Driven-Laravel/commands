{
    "name": "domain-driven-laravel/commands",
    "description": "This package will make artisan commands available but in a domain driven folder structure",
    "license": "MIT",
    "authors": [
        {
            "name": "Menno van Hout",
            "email": "info@mennovanhout.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MennoVanHout\\LaravelDomainDrivenCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MennoVanHout\\LaravelDomainDrivenCommands\\Providers\\DomainDrivenCommandsProvider"
            ]
        }
    }
}
